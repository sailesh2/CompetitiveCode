import math
t=input()
i=0
arr=[]
while i<t:
    x=raw_input()
    x=x.split(' ')
    n=int(x[0])
    m=int(x[1])
    if n==m:
        arr.append(n)
    else:
        if (n%2==0 and m%2!=0)or(n%2!=0 and m%2==0):
            arr.append(2);
        elif n%2==0 and m%2==0:
            arr.append(2)
        else:
            arr.append(3)
    i=i+1
    
i=0
while i<t:
    print arr[i]
    i=i+1
    
