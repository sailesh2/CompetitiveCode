import math
t=input()
i=0
arr=[]
while i<t:
    x=raw_input()
    x=x.split(' ')
    n=int(x[0])
    m=int(x[1])
    a=n-1
    b=m-1
    j=1
    while 1:
        if a%j==0:
            c=a/j
            if b%c==0:
                arr.append(c+1)
                break
        j=j+1
    i=i+1
    
i=0
while i<t:
    print arr[i]
    i=i+1
    
