import math
t=input()
i=0
arr=[]
while i<t:
    x=raw_input()
    x=x.split(' ')
    n=int(x[0])
    m=int(x[1])
    ctr=0
    k=1
    while k<=n:
        j=float((k*(m-1)-(m-n)))/(n-1)
        if j/(int(j))==1:
            if j>=1 and j<=m:
                ctr=ctr+1
        k=k+1
    arr.append(ctr)
    i=i+1
    
i=0
while i<t:
    print arr[i]
    i=i+1
    
